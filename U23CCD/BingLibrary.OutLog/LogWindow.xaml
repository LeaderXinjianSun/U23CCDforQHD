<UserControl x:Class="BingLibrary.OutLog.LogWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BingLibrary.OutLog"
             mc:Ignorable="d"  Initialized="LogWindowInitialized"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <RichTextBox Grid.Row="1" FontSize="16" x:Name="TextLog" Margin="0,10,0,0" IsReadOnly="True" FontFamily="Consolas" />
    </Grid>
</UserControl>